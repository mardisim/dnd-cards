    "lint": { "executor": "@nx/eslint:lint" },
    "storybook": { "options": { "compodoc": true, "compodocArgs": ["-e", "json", "-d", "libs/client/ui"] } },
    "build-storybook": { "options": { "compodoc": true, "compodocArgs": ["-e", "json", "-d", "libs/client/ui"] } }
  }
}
